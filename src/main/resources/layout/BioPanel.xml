<gridBagLayout>
	<component name="name.label">
		<grid x="0" y="0" width="1" height="1" />
		<resize fill="NONE" anchor="WEST" weightx="0" weighty="0.0" />
		<insets top="2" left="2" right="2" bottom="2" />
		<ipad x="0" y="0" />
	</component>
	<component name="name.field">
		<grid x="1" y="0" width="3" height="1" />
		<resize fill="HORIZONTAL" anchor="WEST" weightx="1" weighty="0.0" />
		<insets top="2" left="2" right="2" bottom="2" />
		<ipad x="0" y="0" />
	</component>

	<component name="random.button">
		<grid x="20" y="0" width="1" height="3" />
		<resize fill="BOTH" anchor="EAST" weightx="0" weighty="0.0" />
		<insets top="2" left="2" right="15" bottom="2" />
		<ipad x="0" y="0" />
	</component>
	<component name="clear.button">
